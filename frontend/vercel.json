{
  "version": 2,
  "builds": [
    {
      "src": "server-combined.cjs",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server-combined.cjs"
    },
    {
      "src": "/(.*)",
      "dest": "/server-combined.cjs"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "buildCommand": "npm run build && npm install && npm run build:backend"
}
